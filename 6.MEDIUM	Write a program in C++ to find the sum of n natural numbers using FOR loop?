#include <iostream>
using namespace std;

int main() {
    int n, sum = 0, num;
    cout << "Enter the value of n: "; cin >> n;

    if(n > 0) {
        for (int i = 0; i < n; i++) {
            cin >> num; sum += num;
        }
        cout << "Sum: " << sum << endl;
    } else cout << "Invalid input." << endl;

    return 0;
}
